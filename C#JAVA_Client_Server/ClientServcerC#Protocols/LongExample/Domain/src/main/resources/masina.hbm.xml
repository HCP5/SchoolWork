<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entities.domain">
    <class name="entities.domain.Masina" table="masini">
        <id name="id" type="integer" column="idMasina">
            <generator class="increment"/>
        </id>
        <property name="nrMasina" type="string" column="nrMasina"/>
        <property name="capacitateMotor" type="integer" column="capacitateMotor"/>
    </class>
</hibernate-mapping>
