<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
    <class name="domain.Jucator" table="joc">
        <id name="id" type="integer" column="idJoc">
            <generator class="increment"/>
        </id>
        <property name="idJucator" type="integer" column="idJucator"/>
        <property name="cuvantPrimit" type="string" column="cuvant"/>
        <property name="litereGhicite" type="integer" column="litereghicite"/>
        <property name="puncte" type="integer" column="puncte"/>
        <property name="litere" type="string" column="propuneri"/>
    </class>
</hibernate-mapping>
